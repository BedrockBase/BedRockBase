# Example .env file

# Database Configuration
POSTGRES_HOST="localhost"
POSTGRES_USER="postgres"
POSTGRES_PASSWORD="your_password_here"
POSTGRES_DATABASE="postgres"
POSTGRES_URL="postgres://postgres:your_password_here@localhost:5432/postgres"
POSTGRES_PRISMA_URL="postgres://postgres:your_password_here@localhost:5432/postgres?schema=public"
POSTGRES_URL_NON_POOLING="postgres://postgres:your_password_here@localhost:5432/postgres"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL="your_supabase_url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your_supabase_anon_key"
SUPABASE_URL="your_supabase_url"
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key"
SUPABASE_JWT_SECRET="your_supabase_jwt_secret"

# Application Settings
NODE_ENV="development"